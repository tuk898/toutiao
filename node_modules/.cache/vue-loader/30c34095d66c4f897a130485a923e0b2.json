e>\r\n<script>\r\nimport { mapMutations } from \"vuex\";\r\nimport { UPDATE_USER_INFO } from '../../../vuex/mutationsType'\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            username:\"\",\r\n            password:\"\"\r\n        }\r\n  \r\n    },\r\n\r\n     methods: {\r\n        //  跳转到登录页\r\n        goToUserRegister:function(){\r\n            this.$router.push({\r\n                name:\"usersign\"\r\n            })\r\n        },\r\n            // 1.获取输入框的值\r\n           config:function(){\r\n               let password = this.password;\r\n               let username = this.username;\r\n               if(!password || !username){\r\n                   console.log(1)\r\n                    this.$message({\r\n                        message:\"账号或密码不能为空!\"\r\n                    })\r\n                    return false\r\n               }\r\n               this.$axios.post(\"/loginCheck\",{\r\n                   username,\r\n                   password\r\n               })\r\n               .then(res => {\r\n                   console.log(res)\r\n                  //  this.$message({\r\n                  //      message:res.msg  \r\n                  //  })\r\n                   this.$router.push({\"name\":\"index\"})\r\n                    if(res.status === 0){\r\n              // 登陆成功\r\n              // a. 将用户信息保存到本地 localStorage\r\n              // b. 将用户信息 使用 vuex 存起来 方便实现页面组件间的数据共享\r\n              // c. 跳转到首页\r\n              \r\n              this.$store.commit(UPDATE_USER_INFO,res.wdata)\r\n              // this.$router.push(\"/\"); // 跳转回首页\r\n                      // console.log(UPDATE_USER_INFO)\r\n                     \r\n          }\r\n               })\r\n               .catch(err => err)\r\n           }\r\n        },\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n* {\r\n  padding: 0px;\r\n  margin: 0px;\r\n  box-sizing: border-box;\r\n}\r\n.mask {\r\n  .logout {\r\n     \r\n    width: 440px;\r\n    border: 1px solid #ddd;\r\n    //   display: flex;\r\n    //   justify-content: center;\r\n    //   align-items: center;\r\n    position: absolute;\r\n    transform: translate(-50%, -50%);\r\n    top: 50%;\r\n    left: 50%;\r\n    .top {\r\n      height: 60px;\r\n      width: 100%;\r\n      text-align: center;\r\n      line-height: 60px;\r\n      border-bottom: 1px solid #ddd;\r\n    }\r\n\r\n    .middle {\r\n      height: 50px;\r\n      line-height: 50px;\r\n\r\n      text-align: center;\r\n      input {\r\n        width: 430px;\r\n        border: 1px solid #ddd;\r\n        height: 40px;\r\n        padding: 2px;\r\n        margin-top: 10px;\r\n      }\r\n    }\r\n\r\n    .bottom {\r\n      width: 100%;\r\n      .bottom-dl {\r\n        margin-top: 64px;\r\n        width: 100%;\r\n        height: 50px;\r\n        line-height: 50px;\r\n        text-align: center;\r\n        border-radius: 5px;\r\n        background-color: #ea4245;\r\n      }\r\n\r\n      .bottom-xy {\r\n          display: flex;\r\n          justify-content: space-between;\r\n        .bottom-left {\r\n          a {\r\n               color: blue;\r\n                text-decoration: none;\r\n          }\r\n        }\r\n\r\n        .bottom-right {\r\n          a {\r\n               text-decoration: none;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"]}]}